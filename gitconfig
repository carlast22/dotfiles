[core]
  autocrlf = false
  safecrlf = true
  editor = vim
  excludesfile = ~/.cvsignore

[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:\"%h %ad | %s%d [%an]\" --graph --date=short
  type = cat-file -t
  dump = cat-file -p

[color]
  diff = true
  ui = true

[github]
  user = jreyes33

[push]
  default = tracking
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
